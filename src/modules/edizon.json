{
    "repo":"tomvita/EdiZon-SE",
    "assetRegex":[".*EdiZon.*\\.zip"],
    "customSteps":[
        {
            "action": "extract",
            "path": "temp",
            "source": ".*EdiZon.*\\.zip"
        },
        {
            "action": "createDir",
            "source": "switch/Edizon"
        },
        {
            "action": "move",
            "source": "temp/switch/Edizon/EdiZon.nro",
            "destination": "switch/Edizon/EdiZon.nro"
        },
        {
            "action": "delete",
            "source": "temp"
        }
    ]
}
