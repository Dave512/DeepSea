{
    "repo":"zaksabeast/CaptureSight",
    "assetRegex":[".*CaptureSight.*\\.zip"],
    "customSteps":[
        {
            "action": "createDir",
            "source": "switch/.overlays"
        },
        {
            "action": "createDir",
            "source": "switch/CaptureSight"
        },
        {
            "action": "extract",
            "source": ".*CaptureSight.*\\.zip"
        },
        {
            "action": "move",
            "source": "CaptureSight-Overlay.ovl",
            "destination": "switch/.overlays/CaptureSight-Overlay.ovl"
        },
        {
            "action": "move",
            "source": "CaptureSight.nro",
            "destination": "switch/CaptureSight/CaptureSight.nro"
        }
    ]
}