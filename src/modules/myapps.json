{
    "repo":"qhq/SwitchApp",
    "assetRegex":["nx-ovlloader.zip", "Tesla-Menu.zip", "ovl-sysmodules.zip", "Status-Monitor-Overlay.zip", "DBI.zip", "emuiibo.zip", "EdiZon.zip", "nyx.zip"],
    "customSteps":[
        {
            "action": "createDir",
            "source": "switch/.overlays"
        },
        {
            "action": "createDir",
            "source": "switch/CaptureSight"
        },
        {
            "action": "extract",
            "source": "CaptureSight-Overlay.zip"
        },
        {
            "action": "extract",
            "source": "CaptureSight-Applet.zip"
        },
        {
            "action": "move",
            "source": "CaptureSight-Overlay.ovl",
            "destination": "switch/.overlays/CaptureSight-Overlay.ovl"
        },
        {
            "action": "move",
            "source": "CaptureSight.nro",
            "destination": "switch/CaptureSight/CaptureSight.nro"
        }
    ]
}
